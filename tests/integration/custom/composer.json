{
    "name": "test/console-integration-provider",
    "description": "Integration test project for console command providers functionality",
    "require": {
        "sanmai/console": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../../"
        }
    ],
    "autoload": {
        "psr-4": {
            "TestProviderApp\\": "src/"
        }
    },
    "extra": {
        "console": {
            "provider": "TestProviderApp\\ConsoleProvider"
        }
    }
}
